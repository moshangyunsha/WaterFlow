                let user = new User(rs.getLong(rs.getColumnIndex("ID")), rs.getString(rs.getColumnIndex("USERNAME")), rs.getLong(rs.getColumnIndex("ROLE")), rs.getDouble(rs.getColumnIndex("BALANCE")), "");
