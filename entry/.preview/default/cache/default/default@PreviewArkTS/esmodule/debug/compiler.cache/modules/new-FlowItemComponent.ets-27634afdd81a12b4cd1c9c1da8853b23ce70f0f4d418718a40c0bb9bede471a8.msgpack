          .textOverflow({ overflow: TextOverflow.Ellipsis }) // è¶…å‡ºä¸¤è¡Œæ˜¾ç¤ºçœç•¥å·

        // æŠ˜æ‰£ä¿¡æ¯ (å¯é€‰)
        if (this.item?.discount) {
          Text(this.item?.discount)
            .fontSize($r('app.float.smaller_font_size'))
            .opacity(Const.SIXTY_OPACITY)
            .alignSelf(ItemAlign.Start)
            .margin({ top: 4 })
        }

        // ä»·æ ¼ (æ ¸å¿ƒä¿®æ”¹ï¼šå¤„ç† number ç±»å‹)
        Text(`Â¥${this.item?.price?.toFixed(2) || '0.00'}`)
